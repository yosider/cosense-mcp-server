{
  "name": "cosense-mcp-server",
  "version": "0.1.0",
  "description": "A MCP server for cosense",
  "private": true,
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "inspect": "bunx @modelcontextprotocol/inspector bun run start"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@cosense/std": "npm:@jsr/cosense__std",
    "@modelcontextprotocol/sdk": "^1.4.1",
    "dotenv": "^16.4.7",
    "option-t": "^51.0.1"
  },
  "devDependencies": {
    "@cosense/types": "npm:@jsr/cosense__types",
    "bun-types": "latest",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.4.2"
  }
}
